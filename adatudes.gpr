with "config/adatudes_config.gpr";
project Adatudes is

   for Source_Dirs use ("src");
   for Object_Dir use "obj/" & Adatudes_Config.Build_Profile;
   for Create_Missing_Dirs use "True";
   for Exec_Dir use "bin";
   for Main use ("adatudes.adb");

   package Compiler is
      for Default_Switches ("Ada") use Adatudes_Config.Ada_Compiler_Switches;
   end Compiler;

   package Binder is
      for Switches ("Ada") use ("-Es"); --  Symbolic traceback
   end Binder;

end Adatudes;
